<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient
                    android:angle="45"
                    android:startColor="@color/back_button_gradient_end"
                    android:endColor="@color/back_button_gradient_start"
                    android:type="linear" />
            <corners android:radius="12dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient
                    android:angle="45"
                    android:startColor="@color/back_button_gradient_start"
                    android:endColor="@color/back_button_gradient_end"
                    android:type="linear" />
            <corners android:radius="12dp" />
        </shape>
    </item>
</selector>